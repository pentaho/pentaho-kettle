    <compatible>N</compatible>
    <jsScripts>      <jsScript>        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>
var Report_Template;
var Report_Output;


&#47;&#47; Create full path combining transformation directory + filename
Report_Template = getVariable(&quot;Internal.Transformation.Filename.Directory&quot;,&quot; &quot;)+Report_Template_name;
Report_Output = getVariable(&quot;Internal.Transformation.Filename.Directory&quot;,&quot; &quot;)+Report_Output_name;

&#47;&#47; Trim the leading &apos;file:C&#47;&#47;&#47;&apos; characters from the full path
Report_Template = substr(Report_Template,6);
Report_Output = substr(Report_Output,6);</jsScript_script>
      </jsScript>    </jsScripts>    <fields>      <field>        <name>Report_Template</name>
