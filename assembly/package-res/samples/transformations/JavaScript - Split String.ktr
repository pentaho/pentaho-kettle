<?xml version="1.0" encoding="UTF-8"?>
<transformation>
  <info>
    <name/>
    <directory>/</directory>
    <log>
      <read/>
      <write/>
      <input/>
      <output/>
      <update/>
      <connection/>
      <table/>
      <use_batchid>Y</use_batchid>
      <use_logfield>N</use_logfield>
      </log>
    <maxdate>
      <connection/>
      <table/>
      <field/>
      <offset>0.0</offset>
      <maxdiff>0.0</maxdiff>
      </maxdate>
    <size_rowset>1000</size_rowset>
    <sleep_time_empty>1</sleep_time_empty>
    <sleep_time_full>1</sleep_time_full>
    <dependencies>
      </dependencies>
    </info>
  <notepads>
    </notepads>
  <order>
  <hop> <from>Generate Rows</from><to>Java Script Value</to><enabled>Y</enabled> </hop>  </order>

  <step>
    <name>Generate Rows</name>
    <type>RowGenerator</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
    <fields>
      <field>
        <name>IPAddress</name>
        <type>String</type>
        <format/>
        <currency/>
        <decimal/>
        <group/>
        <nullif>192.168.133.210</nullif>
        <length>15</length>
        <precision>-1</precision>
        </field>
      </fields>
    <limit>10</limit>
    <GUI>
      <xloc>137</xloc>
      <yloc>147</yloc>
      <draw>Y</draw>
      </GUI>
    </step>

  <step>
    <name>Java Script Value</name>
    <type>ScriptValue</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
    <script>
var pieces = IPAddress.getString().split(&quot;\\.&quot;);
var one = pieces[0];
var two = pieces[1];
var three = pieces[2];
var four = pieces[3];
</script>
    <fields>      <field>        <name>one</name>
        <rename>one</rename>
        <type>String</type>
        <length>3</length>
        <precision>-1</precision>
        </field>      <field>        <name>two</name>
        <rename>two</rename>
        <type>String</type>
        <length>3</length>
        <precision>-1</precision>
        </field>      <field>        <name>three</name>
        <rename>three</rename>
        <type>String</type>
        <length>3</length>
        <precision>-1</precision>
        </field>      <field>        <name>four</name>
        <rename>four</rename>
        <type>String</type>
        <length>3</length>
        <precision>-1</precision>
        </field>      </fields>    <GUI>
      <xloc>324</xloc>
      <yloc>148</yloc>
      <draw>Y</draw>
      </GUI>
    </step>

</transformation>
